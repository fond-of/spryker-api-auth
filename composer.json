{
  "name": "fond-of-spryker/api-auth",
  "description": "Zed API auth module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/api": "^0.1.0 || ^0.2.0 || ^0.3.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.14.7",
    "codeception/codeception": "^2.5",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.1",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    }
  }
}
